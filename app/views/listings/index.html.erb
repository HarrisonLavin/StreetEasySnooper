<%= button_to "View As JSON", outbound_path, method: "get" %>

<% @listings.each_with_index do |listing, index| %>
<%= index + 1 %>.<br>
Address: <%= listing.address %><br>
Unit: <%= listing.unit %><br>
Price: <%= number_to_currency(listing.price) %><br>
Type of Listing: <%= listing.listing_class %><br>
<%= link_to("Find Out More Here", listing.url )%> <br> 
<br>
<% end %>